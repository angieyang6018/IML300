{
  "name": "Chrome bubbles extension",
  "manifest_version": 2,
  "version": "1.0",
  "description": "p5js turn webpage into an aquarium with bouncing bubbles!",
  "icons": {
    "128": "icon/bubbles.png"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["js/p5.min.js", "js/sketch.js"],
      "css": ["css/style.css"]
    }
  ]
}
